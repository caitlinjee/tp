@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":WishfulShrinkingParser" as WishfulShrinkingParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedWishfulShrinking" as VersionedWishfulShrinking MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> WishfulShrinkingParser : parseCommand(undo)
activate WishfulShrinkingParser

create UndoCommand
WishfulShrinkingParser -> UndoCommand
activate UndoCommand

UndoCommand --> WishfulShrinkingParser
deactivate UndoCommand

WishfulShrinkingParser --> LogicManager : u
deactivate WishfulShrinkingParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoWishfulShrinking()
activate Model

Model -> VersionedWishfulShrinking : undo()
activate VersionedWishfulShrinking

VersionedWishfulShrinking -> VersionedWishfulShrinking :resetData(ReadOnlyWishfulShrinking)
VersionedWishfulShrinking --> Model :
deactivate VersionedWishfulShrinking

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
